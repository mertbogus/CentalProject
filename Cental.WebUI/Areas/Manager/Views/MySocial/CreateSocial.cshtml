@model CreateUserSocialDto
@{
    ViewData["Title"] = "CreateSocial";
    Layout = "~/Areas/Manager/Views/ManagerLayout/Index.cshtml";
}

<form method="post">
    <label>Sosyal Medya Adı</label>
    <input type="text" asp-for="Title" class="form-control mb-2" />
    <label>Sosyal Medya İcon</label>
    <input type="text" asp-for="Icon" class="form-control mb-2" />
    <label>Sosyal Medya Url</label>
    <input type="text" asp-for="Url" class="form-control mb-2" />
    <button class="btn btn-outline-dark">Kaydet</button>
</form>

